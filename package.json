{
  "name": "uttype",
  "version": "0.2.1",
  "packageManager": "pnpm@8.6.1",
  "description": "Utility types library.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/uttype.git"
  },
  "keywords": [
    "types",
    "utility"
  ],
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "prepublishOnly": "pnpm run test",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "test": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.39.5",
    "@type-challenges/utils": "^0.1.1",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "lint-staged": "^13.2.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  }
}
