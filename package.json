{
  "name": "uttype",
  "version": "0.7.1",
  "packageManager": "pnpm@8.10.0",
  "description": "Tiny helper library with some useful functions / types.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/uttype.git"
  },
  "keywords": [
    "types",
    "utility",
    "tiny",
    "helper"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm run test && tsup",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "test": "tsc --noEmit && vitest --run",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.43.1",
    "@type-challenges/utils": "^0.1.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "lint-staged": "^15.0.2",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  }
}
