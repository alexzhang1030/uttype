{
  "name": "uttype",
  "version": "0.3.3",
  "packageManager": "pnpm@8.6.11",
  "description": "Tiny helper library with some useful functions / types.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/uttype.git"
  },
  "keywords": [
    "types",
    "utility",
    "tiny",
    "helper"
  ],
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "prepublishOnly": "pnpm run test",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "test": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.39.8",
    "@type-challenges/utils": "^0.1.1",
    "bumpp": "^9.1.1",
    "eslint": "^8.46.0",
    "lint-staged": "^13.2.3",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  }
}
